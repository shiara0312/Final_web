<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    <link rel="shortcut icon" href="@Url.Content("~/Content/img/favicon.png")">
    <link rel="apple-touch-icon" href="@Url.Content("~/Content/img/apple-touch-icon.png")">
    <link rel="apple-touch-icon" sizes="72x72" href="@Url.Content("~/Content/img/apple-touch-icon-72x72.png")">
    <link rel="apple-touch-icon" sizes="114x114" href="@Url.Content("~/Content/img/apple-touch-icon-114x114.png")">
    <title>Full Spoon - Restaurant Template </title>
    <!-- Bootstrap Core CSS -->
    <link href="@Url.Content("~/Content/css/bootstrap.min.css")" rel="stylesheet">
    <!-- GA -->
    <script src="@Url.Content("~/Content/js/ga.js")"></script>
    <!-- Full Spoon Custom CSS -->
    <link href="@Url.Content("~/Content/css/full-spoon.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/animate-css/animate.css")" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/owl-carousel/owl.carousel.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/owl-carousel/owl.theme.css")">
    <!-- Full Spoon Custom Fonts -->
    <link href="@Url.Content("~/Content/font-awesome/css/font-awesome.min.css")" rel="stylesheet" type="text/css">
    <link href='https://fonts.googleapis.com/css?family=Catamaran:400,600,300,700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Amatic+SC:400,700' rel='stylesheet' type='text/css'>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Mi aplicación ASP.NET</p>
        </footer>
    </div>
    <script src="@Url.Content("~/Content/js/jquery.js")"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="@Url.Content("~/Content/js/bootstrap.min.js")"></script>

    <!-- Plugin JavaScript -->
    <script src="@Url.Content("~/Content/js/isotope.pkgd.min.js")"></script>
    <script src="@Url.Content("~/Content/magnific-popup/jquery.magnific-popup.min.js")"></script>
    <script src="@Url.Content("~/Content/../cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js")"></script>
    <script src="@Url.Content("~/Content/js/classie.js")"></script>
    <script src="@Url.Content("~/Content/js/cbpAnimatedHeader.min.js")"></script>
    <script src="@Url.Content("~/Content/js/wow.min.js")"></script>
    <script src="@Url.Content("~/Content/owl-carousel/owl.carousel.js")"></script>

    <!-- Custom Theme JavaScript -->
    <script src="@Url.Content("~/Content/js/fullspoon.js")"></script>

    <!-- Contact Form JavaScript -->
    <script src="@Url.Content("~/Content/js/jqBootstrapValidation.js")"></script>
    <script src="@Url.Content("~/Content/js/contact_me.js")"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
